{
  "scriptFile": "__init__.py",
  "bindings": [

    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "message",
      "type": "table",
      "tableName": "mytesttable",
      "partitionKey": "mytesttable",
      "rowKey": "{rand-guid}",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "msg",
      "queueName": "outqueue",
      "connection": "AzureWebJobsStorage"
    }
  ]
}